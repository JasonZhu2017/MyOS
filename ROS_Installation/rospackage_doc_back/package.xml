	<!-- They should never duplicate any dependencies already mentioned as build -->
	<!-- or run dependencies.-->
	<test_depend></test_depend>

	<!-- Documentation Tool Dependencies specify documentation tools -->
	<!-- which this package needs to generate documentation. -->
	<doc_depend></doc_depend>
</package>
