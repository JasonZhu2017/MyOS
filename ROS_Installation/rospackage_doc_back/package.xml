<package format="2">
	<name>hello_test</name>
	<version>1.0.0</version>
	<description>
		This is the "hello-world" test programme.
	</description>
	<maintainer email="gongzheng01@baidu.com">gongzheng</maintainer>
	<license>TODO</license>

	<!-- <url type="website">http://</url> -->
